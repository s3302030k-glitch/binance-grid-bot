# Binance API Configuration
# Get your API keys from:
# - Testnet: https://testnet.binance.vision/
# - Mainnet: https://www.binance.com/en/my/settings/api-management

BINANCE_API_KEY=your_api_key_here
BINANCE_API_SECRET=your_api_secret_here

# Testnet Configuration
# Set to True for Binance Testnet (https://testnet.binance.vision)
# Set to False for Binance Mainnet (real trading)
# IMPORTANT: Testnet only supports SPOT market (not futures)
USE_TESTNET=True

# Logging Configuration
LOG_LEVEL=INFO

# Telegram Notifications (Optional)
# Get bot token from @BotFather on Telegram
# Get chat ID by messaging your bot and visiting: https://api.telegram.org/bot<YOUR_BOT_TOKEN>/getUpdates
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here
